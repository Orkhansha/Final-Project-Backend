@model List<Product>


@foreach (var item in Model)
{
    <li style="text-align: center;list-style: none;background-color: black;">
        <a asp-controller="product" asp-action="detail" asp-route-id="@item.Id" style="color:whitesmoke; text-decoration:none;">
            @item.Title
        </a>
    </li>
}
